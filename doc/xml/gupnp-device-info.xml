<indexterm zone="gupnp-device-info-get-description-value"><primary sortas="gupnp_device_info_get_description_value">gupnp_device_info_get_description_value</primary></indexterm><programlisting><link linkend="char">char</link> *              gupnp_device_info_get_description_value
